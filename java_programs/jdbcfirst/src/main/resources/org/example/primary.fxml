<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PrimaryController">
    <children>
        <Label text="login">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <TextField fx:id="un" promptText="username"/>
        <TextField fx:id="pw" promptText="password"/>

        <Button fx:id="primaryButton" onAction="#switchToSecondary" text="login">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </Button>
        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="197.0"
               textAlignment="CENTER">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
