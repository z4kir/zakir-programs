                <Label prefHeight="30.0" prefWidth="127.0" text="Name :" VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets top="20.0"/>
                    </VBox.margin>
                </Label>
                <Label prefHeight="30.0" prefWidth="150.0" text="Mobile:"/>
                <Label prefHeight="30.0" prefWidth="150.0" text="Email:"/>
            </children>
        </VBox>
        <VBox layoutX="150.0" prefHeight="300.0" prefWidth="242.0" spacing="22.0" AnchorPane.rightAnchor="8.0">
            <children>
                <TextField fx:id="namescreen2" prefHeight="25.0" prefWidth="50.0" VBox.vgrow="ALWAYS">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets top="25.0"/>
                    </VBox.margin>
                </TextField>
                <TextField fx:id="mobilescreen2">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </TextField>
                <TextField fx:id="emailscreen2">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </TextField>
                <Label alignment="CENTER" prefHeight="88.0" prefWidth="244.0" style="-fx-background-color: yellow;"
