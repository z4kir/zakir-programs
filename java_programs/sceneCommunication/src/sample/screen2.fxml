                        <Font name="System Bold Italic" size="24.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
        <Button layoutX="80.0" layoutY="381.0" mnemonicParsing="false" onAction="#screen3window" prefHeight="25.0"
                prefWidth="197.0"
                style="-fx-background-color: transparent; -fx-background-radius: 30px; -fx-border-color: blue; -fx-border-radius: 30px;"
                text="screen 3"/>
        <Button layoutX="80.0" layoutY="342.0" mnemonicParsing="false" onAction="#screen1window" prefHeight="25.0"
                prefWidth="197.0"
                style="-fx-background-color: transparent; -fx-border-color: blue; -fx-background-radius: 30px; -fx-border-radius: 30px;"
                text="screen 1"/>
