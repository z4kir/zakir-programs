<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <TextField fx:id="username_set" layoutX="87.0" layoutY="89.0" promptText="username"/>
        <TextField fx:id="password_set" layoutX="87.0" layoutY="144.0" promptText="password"/>
        <Button fx:id="submit" layoutX="102.0" layoutY="199.0" mnemonicParsing="false" onAction="#set" text="Submit"/>
    </children>
</AnchorPane>
